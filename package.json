{
  "name": "api-core",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --bail --runInBand --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch --runInBand --detectOpenHandles --forceExit",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "migration:generate": "npm run typeorm -- migration:generate -f ormconfig -d src/database/migrations -n",
    "migration:create": "npm run typeorm -- migration:create -f ormconfig -d src/database/migrations -n",
    "migration:run": "npm run typeorm -- migration:run -f ormconfig",
    "migration:revert": "npm run typeorm -- migration:revert -f ormconfig",
    "schema:drop": "npm run typeorm -- schema:drop",
    "schema:sync": "npm run typeorm -- schema:sync",
    "schema:reset": "npm run schema:drop && npm run schema:sync",
    "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed -n src/ormconfig.ts",
    "scripts:typeorm": "npx node ./node_modules/typeorm/cli.js --config ./dist/ormconfig.js",
    "scripts:migration:run": "npm run scripts:typeorm -- migration:run",
    "scripts:seed:run": "npx node ./node_modules/typeorm-seeding/dist/cli.js seed -n ./dist/ormconfig.js",
    "scripts:seed:run:prod": "NODE_ENV=production npm run scripts:seed:run",
    "scripts:runHelloWorld": "npx node ./dist/scripts/hello-world",
    "seed:reset": "npm run schema:reset && npm run seed:run"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.4.2",
    "@nestjs/bull": "^0.1.1",
    "@nestjs/common": "^7.0.8",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.8",
    "@nestjs/cqrs": "^6.1.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/microservices": "^7.0.8",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.8",
    "@nestjs/swagger": "^4.5.7",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjsx/crud": "^4.6.0",
    "@nestjsx/crud-typeorm": "^4.6.0",
    "aws-sdk": "^2.657.0",
    "bcrypt": "~3.0.8",
    "body-parser": "^1.19.0",
    "bull": "^3.14.0",
    "chalk": "^2.4.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "cls-hooked": "^4.2.2",
    "dotenv": "~8.2.0",
    "dotenv-override": "^5.0.1",
    "form-data": "^3.0.0",
    "geolib": "^3.3.1",
    "handlebars": "^4.7.6",
    "helmet": "^3.21.3",
    "i18next": "^18.0.1",
    "i18next-express-middleware": "^1.7.1",
    "i18next-icu": "^1.0.1",
    "i18next-node-fs-backend": "^2.1.1",
    "ioredis": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.24.1",
    "moment": "^2.25.3",
    "nanoid": "^3.1.10",
    "nodemailer": "^6.4.10",
    "odata-client": "https://github.com/alphacode-my/odata-client.git",
    "onesignal-api-client-nest": "^1.0.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.2",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "soap": "^0.32.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.24",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.13.0",
    "@types/express": "^4.17.3",
    "@types/faker": "^4.1.11",
    "@types/helmet": "0.0.45",
    "@types/jest": "25.1.4",
    "@types/node": "^13.11.1",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.2.4",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "@shared/(.*)": "<rootDir>/shared/$1",
      "@cqrs/(.*)": "<rootDir>/cqrs/$1"
    },
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.ts": [
      "prettier --cache --write",
      "eslint --cache --fix"
    ]
  }
}
